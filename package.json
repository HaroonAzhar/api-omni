{
    "name": "omni-api",
    "version": "0.2.0",
    "description": "Api for OMNI project",
    "author": "uCreate",
    "license": "MIT",
    "scripts": {
        "build": "yarn run build-ts && yarn run lint",
        "build-ts": "tsc",
        "db:createMigration": "ts-node ./node_modules/.bin/knex --knexfile knexfile.ts -x ts migrate:make",
        "db:createSeed": "ts-node ./node_modules/.bin/knex --knexfile knexfile.ts -x ts seed:make",
        "db:migrate": "ts-node ./node_modules/.bin/knex --knexfile knexfile.ts migrate:latest",
        "db:rollback": "ts-node ./node_modules/.bin/knex --knexfile knexfile.ts migrate:rollback",
        "db:runSeed": "ts-node ./database/seeds/index.ts",
        "debug": "yarn run build && yarn run watch-debug",
        "deploy": "yarn install && yarn run nest:build && yarn run knex migrate:latest && yarn run ts-node ./database/seeds/index.ts",
        "docs": "apidoc -i ./src -o dist/apidoc/",
        "knex:dev": "ts-node ./node_modules/knex/bin/cli.js",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "monitor": "pm2 monit",
        "nest:build": "node node_modules/@nestjs/cli/bin/nest.js build",
        "nest:dev": "node node_modules/@nestjs/cli/bin/nest.js start --watch",
        "serve": "node dist/main.js",
        "serve-debug": "nodemon --inspect dist/main.js",
        "start": "pm2 start ecosystem.config.js",
        "test": "jest --forceExit --verbose",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
        "watch-node": "nodemon dist/main.js",
        "watch-test": "yarn run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "_moduleAliases": {
        "@v2": "dist/v2/"
    },
    "dependencies": {
        "@hapi/joi": "^16.1.8",
        "@nestjs/cli": "^8.1.1",
        "@nestjs/common": "^8.0.6",
        "@nestjs/core": "^8.0.6",
        "@nestjs/cqrs": "^8.0.0",
        "@nestjs/platform-express": "^8.0.6",
        "@types/colors": "^1.2.1",
        "@types/cors": "^2.8.6",
        "@types/faker": "^4.1.11",
        "@types/inflected": "^1.1.29",
        "@types/joi": "^14.3.4",
        "@types/jsonapi-serializer": "^3.6.2",
        "@types/uuid": "^3.4.6",
        "apidoc": "^0.19.0",
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.12.2",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-validator": "^6.2.0",
        "faker": "^4.1.0",
        "firebase-admin": "^9.3.0",
        "inflected": "^2.0.4",
        "joi": "^14.3.1",
        "jsonapi-serializer": "^3.6.5",
        "knex": "^0.20.2",
        "lodash": "^4.17.15",
        "long": "4.0.0",
        "lusca": "^1.6.1",
        "moment": "^2.29.1",
        "mssql": "^6.0.1",
        "pm2": "^4.4.0",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "rollbar": "^2.14.4",
        "rxjs": "^7",
        "tsyringe": "^4.3.0",
        "uuid": "^3.3.3",
        "winston": "^3.2.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "src/**/*.{js,ts}": [
            "eslint"
        ]
    },
    "optionalDependencies": {},
